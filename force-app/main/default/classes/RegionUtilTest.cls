    @IsTest
    public static void testRegionForInvalidCountryCode() {
        System.assert(RegionUtil.getRegionByCountryCode(null) == 'Other', 'Invalid region for null.');
        System.assert(RegionUtil.getRegionByCountryCode('') == 'Other', 'Invalid region for empty string.');
        System.assert(RegionUtil.getRegionByCountryCode(',') == 'Other', 'Invalid region for invalid country code.');
        System.assert(RegionUtil.getRegionByCountryCode('ABC') == 'Other', 'Invalid region for invalid country code.');
    }
    
